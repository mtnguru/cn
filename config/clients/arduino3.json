{
  "name": "arduino3",
  "model": "arduino",
  "sampleInterval": 1000,
  "telegraf": "cabin_cn",
  "subscribeTopics": {
    "output": "PROJECT/output/control/MY_CLIENT_NAME",
    "config": "PROJECT/config/post/MY_IP",
    "admin":  "PROJECT/admin/+/MY_CLIENT_NAME"
  },
  "inputs": {
    "I_arduino3_K_LivingRoom_C": {
      "type": "MAX6675",
      "channels": "2,4,5",
      "desc": "Max 6675 thermocouple board"
    }
  },
  "outputs": {
    "O_arduino3_LED_Onboard_On": {
      "type": "digital",
      "channel": "16",
      "desc": "Arduino onboard LED"
    }
  }
}
